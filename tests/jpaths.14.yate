/// {
///     description: 'Compare nodeset to nodeset and nodeset to scalar',
///     data: {
///         foo: [ 'first', 'second', 'third' ],
///         bar: [ 'one', 'two', 'three' ],
///         boo: [ 'first', 'two', 'three' ]
///     },
///     result: ".foo == 'second'.foo == .boo"
/// }

match / {
    if .foo == "second" {
        ".foo == 'second'"
    }

    if .bar == "second" {
        ".bar == 'second'"
    }

    if .foo == .bar {
        ".foo == .bar"
    }

    if .foo == .boo {
        ".foo == .boo"
    }
}

