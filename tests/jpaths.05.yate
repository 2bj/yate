/// {
///     "description": "apply . #mode",
///     "yate": "jpaths.05.yate",
///     "result": "<ul><li>First</li><li>Second</li><li>Third</li></ul>"
/// }

match / {
    <ul>
        apply .items.item
    </ul>
}

match .item {
    <li>
        apply . content
    </li>
}

match .item content {
    .title
}

