/// {
///     "description": "dots in jpath",
///     "result": "/ left / user / content / app / right / user / content /",
///     "known": true
/// }

match / {
    apply .blocks.app.content.user.left
}

match .left {
    "/ "
    name(.)
    " / "
    name(..)
    " / "
    name(...)
    " / "
    name(....)
    " / "
    name(..right)
    " / "
    name(...user)
    " / "
    name(....content)
    " /"
}

