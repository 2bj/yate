/// {
///     "description": "predicate in match",
///     "yate": "match.01.yate",
///     "result": "<ul><li>First</li><li class=\"disabled\">Second</li><li>Third</li></ul>"
/// }

match / {
    <ul>
        apply .items.item
    </ul>
}

match .items.item {
    <li>{ .title }</li>
}

match .items.item[ .disabled ] {
    <li class="disabled">{ .title }</li>
}

